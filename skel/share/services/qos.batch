check -strong qos.service.verification.timeout.unit

create org.dcache.cells.UniversalSpringCell ${qos.cell.name} \
        "classpath:org/dcache/qos/qos.xml \
        -subscribe=${qos.cell.subscribe} \
        -consume=${qos.cell.consume}"
